      "EngineConfig-Output": {
        "properties": {
          "server": {
            "$ref": "#/components/schemas/ServerConfig"
          },
          "agent": {
            "$ref": "#/components/schemas/AgentConfig-Output"
          }
        },
        "type": "object",
        "required": [
          "agent"
        ],
        "title": "EngineConfig",
        "description": "Main engine configuration model for the entire Idun Agent Engine."
